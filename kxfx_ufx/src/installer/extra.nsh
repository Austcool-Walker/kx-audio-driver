; macros

!macro DA_PLUGIN_REG PATH ID GUID
    WriteRegStr HKCU "${KXPLUGINS_KEY}" "${GUID}" "${PATH}" 
    WriteRegStr HKCU "${KXPLUGINS_KEY}" "${GUID}.name" "${ID}"
    !insertmacro DEFINE PLUGIN_GUID "${GUID}"
!macroend

!macro DA_PLUGIN_UNREG PATH ID GUID
    DeleteRegValue HKCU "${KXPLUGINS_KEY}" "${GUID}" 
    DeleteRegValue HKCU "${KXPLUGINS_KEY}" "${GUID}.name"
!macroend

!macro DA_PRESET_REG TITLE DATA
    !if ${CMD} == DA_PLUGIN_REG
        WriteRegBin HKCU "${KXPLUGINS_KEY}\${PLUGIN_GUID}" "${TITLE}" ${DATA}
    !endif
!macroend

